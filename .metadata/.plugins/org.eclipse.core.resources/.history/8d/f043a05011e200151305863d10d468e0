<!-- mapper代理用XML -->
<mapper namespace="com.example.mapper.OrdersMapperCustom">
	
   <select id="findOedersUser" 
		resultType="com.example.domain.po.OrdersCustom">
		select orders.*,user.username,user.sex,user.address 
		from orders,user  where orders.user_id = user.id;
	</select>
</mapper>
